{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs/(.*)",
      "dest": "src/main.ts",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate",
        "Content-Type": "text/javascript; charset=utf-8"
      }
    },
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
