{
  "routes": [
    { "src": "/styles.css", "dest": "/styles.css" },
    { "src": "/robots.txt", "dest": "/robots.txt" },
    { "src": "/sitemap.xml", "dest": "/sitemap.xml" },
    { "src": "/assets/(.*)", "dest": "/images/$1" },
    { "src": "/projects/(.*)", "dest": "/projects/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
